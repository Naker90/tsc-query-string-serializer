{
  "name": "tsc-query-string-serializer",
  "version": "1.0.0",
  "description": "",
  "main": "src/queryStringSerializer.ts",
  "contributors": [
    "Antonio SÃ¡nchez <antoniojesussg96@gmail.com>"
